@model List<Student>
@{
    ViewBag.Title = "Housing Posts";
}
<div class="row">
    <div class="col-sm-10">
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Phone</th>
                    <th>Email</th>
                    <th>Dob</th>
                    @*<th></th>*@
                </tr>
            </thead>
            <tbody>
                @foreach (var s in Model)
                {
                    <tr>
                        <td>@s.StudentID</td>
                        <td>@s.Name</td>
                        <td>@s.Phone</td>
                        <td>@s.Email</td>
                        <td>@s.Dob</td>

                        <td>                           
                               @* <a asp-controller="Product" asp-action="Details" asp-route-id=@p.ProductID class="btn btn-info"> Details </a>
                                <a asp-controller="Product" asp-action="Edit" asp-route-id=@p.ProductID class="btn btn-info"> Edit </a>*@
                                <a asp-controller="" asp-action="Delete" asp-route-id="@s.StudentID" class="btn btn-info"> Delete </a>

                        </td>
                    </tr>
                }
            </tbody>
        </table>
        
    </div>
</div>

 <form asp-action="Add">
     <button type="submit" class="btn btn-info"> Create Listings </button>
 </form>
                


                       
                  
                    


   

