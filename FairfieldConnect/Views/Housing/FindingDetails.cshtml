@model FindPost
@{
    ViewBag.Title = "Details";
}

<h3>Finding Details</h3>
<div class="row" style="width:800px">      
        <table>
            <tr>
                <td>Title:</td>
                <td>@Model.Title</td>
            </tr>
            <tr>
                <td>Description:</td>
                <td>@Model.Desc</td>
            </tr>
            <tr>
                <td>Location:</td>
                <td>@Model.Location</td>
            </tr>
            <tr>
                <td>Rate:</td>
                <td>@Model.Rate</td>
            </tr>
            <tr>
                <td>Category:</td>
                <td>@Model.Category.Name</td>
            </tr>
            <tr>
                <td>Created by:</td>
                <td>@Model.Student.Name</td>
            </tr>          
        </table>
        <h4>Interested?</h4>
        <h4>Contact Information</h4>
        <table>
            <tr>
                <td>Name:</td>
                <td>@Model.Student.Name</td>           
            </tr>
            <tr>
                <td>Phone:</td>
                <td>@Model.Student.Phone</td>           
            </tr>
             <tr>
                <td>Email:</td>
                <td>@Model.Student.Email</td>           
            </tr>
        </table>
                
        <button asp-controller="Housing" asp-action="Find" class="btn btn-warning">Cancel</button>
</div>

