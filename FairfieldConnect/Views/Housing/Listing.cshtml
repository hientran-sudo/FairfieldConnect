@model List<ListPost>
@{
    ViewBag.Title = "Listings";
}
<div class="row">
    <div>
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Title</th>
                    <th>Description</th>
                    <th>Address</th>
                    <th>Rate</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>

                @foreach (var r in Model)
                {
                    <tr>
                        <td>@r.ListPostID</td>
                        <td>@r.Title</td>
                        <td>@r.Desc</td>
                        <td>@r.Address</td>
                        <td>@r.Rate</td>
                        <td>
                            <a asp-controller="Housing" asp-action="ListingDetails" asp-route-id=@r.ListPostID class="btn btn-info"> Details </a>
                                                         
                        </td>
                    </tr>
                }
            </tbody>
        </table>        
    </div>
</div>

<br/>
 <form asp-action="AddListing">
     <button type="submit" class="btn btn-info"> Create a Listing </button>
 </form>
                


                       
                  
                    


   

